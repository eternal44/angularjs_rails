<div class="row" ng-app="AngulaRails" ng-controller="HelloWorldController">
  <div class="row" ng-app>
    <div class="col-md-6">
      <h1>Hello AngularJS</h1>
      <p>Type in the input box and watch as Angular updates through binding.</p>
      <input type="text" class="form-control" ng-model="myText" >
      {{ myText }}
    </div>
  </div>
</div>
